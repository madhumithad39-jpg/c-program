#include<stdio.h>
int pallidrome(int n) {
    int rev=0;
    int s=n;
    while(s>0) {
        rev=rev*10+s%10;
        s=s/10;
    }
    if(rev==n) {
        printf("Pallidrome");
    }
    else {
        printf("No Pallidrome");
    }
    return rev;
}
int main() {
    int n;
    scanf("%d",&n);
    pallidrome(n);
    return 0;
}
