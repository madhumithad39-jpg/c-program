#include<stdio.h>
int fib(int n) {
    int a=0,b=1,c;
    if (n>=1) {
        printf("%d",a);
    }
    if (n>=2) {
        printf("%d",b);
    }
    for (int i=3;i<=n;i++) {
        c=a+b;
        a=b;
        b=c;
        printf("%d",c);
    }
}
int main() {
    int n;
    scanf("%d",&n);
    fib(n);
    return 0;
}
