#include<stdio.h>
#include<string.h>
int main() {
    char s[100];
    int len=0;
    int palin=1;
    scanf("%s", s);
    for(int i=0;s[i]!='\0';i++) {
        len++;
    }
    for (int i=0;i<len/2;i++) {
        if (s[i]!=s[len-1-i]) {
            palin=0;
            break;
        }
    }
    if (palin==1) {
        printf("Palindrom");
    }
    else {
        printf("Not Palindrom");
    }
    return 0;
}
