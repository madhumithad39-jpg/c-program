#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int count = 0;
    fgets(s, sizeof(s), stdin);\
    for(int i = 0; s[i] != '\0'; i++) {
        if(s[i] == ' ') {
            count++;
        }
    }
    if(s[0] != '\0' && s[0] != '\n') {
        count = count + 1;
    }
    printf("%d", count);
    return 0;
}
