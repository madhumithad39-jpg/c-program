#include<stdio.h>
#include<string.h>
int main() {
    char pass[100];
    int upper=0;
    scanf("%s", &pass);
    for (int i=0;pass[i]!= '\0';i++) {
        if (pass[i] >= 'A' && pass[i] <= 'Z') {
            upper=1;
            break;
        }
    }
    if (upper==1) {
        printf("Valid");
    }
    else {
        printf("Invalid");
    }
    return 0;
}
