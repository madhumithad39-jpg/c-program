#include<stdio.h>
int main() {
    int n, reversed = 0, temp;
    scanf("%d", &n);
    temp = n;
    for (; temp > 0; temp /= 10) {
        int digit = temp % 10;
        reversed = reversed * 10 + digit;
    }
    if (reversed == n) {
        printf("Yes");
    } else {
        printf("No");
    }
}
