#include <stdio.h>
int main() {
    int N;
    scanf("%d", &N);
    int transactions[N];
    for(int i = 0; i < N; i++) {
        scanf("%d", &transactions[i]);
    }
    int i = 0;
    int highValueCount = 0;
    int consecutiveCount = 0;
    int fraudAttempt = -1;
    while(i < N) {
        if(transactions[i] >= 50000) {
            highValueCount++;
            consecutiveCount++;
        } else {
            consecutiveCount = 0;
        }
        
        if(consecutiveCount == 3 && fraudAttempt == -1) {
            fraudAttempt = i + 1; 
        }
        i++;
    }
    if(fraudAttempt != -1) {
        printf("Fraud Triggered At Attempt: %d\n", fraudAttempt);
    } else {
        printf("Fraud Triggered At Attempt: Not Triggered\n");
    }

    printf("High-Value Transactions: %d\n", highValueCount);
    return 0;
}
