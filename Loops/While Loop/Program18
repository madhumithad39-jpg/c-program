#include <stdio.h>
int main() {
    int capacity,n; 
    int load;
    int safe = 0, failure = 0;
    int i=0;
    scanf("%d %d", &capacity, &n);
    while (i<n) {
        scanf("%d", &load);
        if (load <= capacity)
            safe++;
        else
            failure++;
    }
    printf("Safe Hours: %d\n", safe);
    printf("Failure Count: %d", failure);
    return 0;
}
