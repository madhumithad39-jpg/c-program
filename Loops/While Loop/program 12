#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int i = 0;
    int p;
    int max = 0;
    int surge = 0;
    while (i < n) {
        scanf("%d", &p);
        if (p > max) {
            max = p;
        }
        if (p > 5) {
            surge++;
        }
        i++;
    }
    printf("Max Usage: %d\n", max);
    printf("Surge Hours: %d", surge);
    return 0;
}
