#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int i = 0;
    int h;
    int inactive = 0;

    while (i < n) {
        scanf("%d", &h);

        if (h == 0) {
            inactive++;
        }
        i++;
    }
    if (inactive >= 3) {
        printf("Risk Status: High");
    } else {
        printf("Risk Status: Low");
    }
    printf("Inactive Weeks: %d\n", inactive);
    return 0;
}
