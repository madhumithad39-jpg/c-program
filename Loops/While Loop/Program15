#include <stdio.h>
int main() {
    int n, hours;
    int total = 0, burnout = 0;
    int i=0;
    scanf("%d", &n);
    while (i<n) {
        scanf("%d", &hours);
        total=total+ hours;
        if (hours > 4) {
            burnout++;
        }
            i++;
    }
    printf("Total Overtime: %d\n", total);
    printf("Burnout Days: %d", burnout);
    return 0;
}
