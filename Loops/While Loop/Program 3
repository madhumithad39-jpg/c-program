#include <stdio.h>
int main() {
    int total, n;
    int usage;
    int i= 0;
    scanf("%d", &total);
    scanf("%d", &n);
    while (i < n && total > 0) {
        scanf("%d", &usage);
        total = total - usage;
        i++;
    }
    if (total< 0) {
        total = 0;
    }
    printf("Days Used: %d\n", i);
    printf("Remaining Data: %dGB\n", total);
    return 0;
}
