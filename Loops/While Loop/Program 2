#include<stdio.h>
int main() {
    int n,i=0;
    int b,t;
    int low=0;
    scanf("%d%d",&n,&b);
    while (i<n) {
        scanf("%d",&t);
        b=b+t;
        if (b<2000) {
            low++;
        }
        i++;
    }
    printf("Final Balance: %d\n", b);
    printf("Low Balance Days: %d\n", low);
return 0;
}
