#include <stdio.h>
int main() {
    int data, n, use, day = 0;
    scanf("%d", &data);
    scanf("%d", &n);
    while (day < n && data > 0) {
        scanf("%d", &use);
        data =data-use;
        day++;
    }
    if (data > 0) {
        printf("Exhausted Day: Not Exhausted\n");
        printf("Overused Data: 0");
    } else {
        printf("Exhausted Day: %d\n", day);
        if (data < 0)
            printf("Overused Data: %d", -data);
        else
            printf("Overused Data: 0");
    }
    return 0;
}
