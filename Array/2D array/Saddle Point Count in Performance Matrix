#include <stdio.h>
int main () {
    int r,c;
    scanf("%d %d", &r, &c);
    int arr[r][c];
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &arr[i][j]);
        }
    }
    int count=0;
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            int rmin=1;
            int cmax=1;
            for (int k = 0; k < c; k++) {
                if (arr[i][j]>arr[i][k]) {
                    rmin=0;
                }
            }
            for (int k=0; k<r; k++) {
                if (arr[i][j]<arr[k][j]) {
                    cmax=0;
                }
            }
            if (rmin) {
                if (cmax) {
                    count++;
                }
            }
        }
    }
    printf("%d", count);
    return 0;
}
