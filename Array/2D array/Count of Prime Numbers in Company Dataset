#include <stdio.h>
int main() {
    int r, c;
    scanf("%d %d", &r, &c);
    int a[r][c];
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    int count = 0;
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            int div = 0;
            for (int k = 1; k <= a[i][j]; k++) {
                if (a[i][j] % k == 0) {
                    div++;
                }
            }
            if (div == 2) {
                count++;
            }
        }
    }
    printf("%d", count);
    return 0;
}
