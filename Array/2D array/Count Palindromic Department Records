#include <stdio.h>
int main() {
    int r, c;
    scanf("%d %d", &r, &c);
    int a[100][100];
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    int count = 0;
    for (int i = 0; i < r; i++) {
        int match = 0;
        for (int j = 0; j < c / 2; j++) {
            if (a[i][j] != a[i][c - 1 - j]) {
                match++;
            }
        }
        if (match == 0) {
            count++;
        }
    }
    printf("%d", count);
    return 0;
}
