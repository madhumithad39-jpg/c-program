#include <stdio.h>
int main() {
    int r, c;
    scanf("%d %d", &r, &c);
    int a[r][c];
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    int count = 0;
    for (int i = 0; i < r; i++) {
        int dup = 0;
        for (int j = 0; j < c; j++) {
            for (int k = j + 1; k < c; k++) {
                if (a[i][j] == a[i][k]) {
                    dup++;
                }
            }
        }
        if (dup == 0) {
            count++;
        }
    }
    printf("%d", count);
    return 0;
}
