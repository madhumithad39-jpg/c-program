#include<stdio.h>
int main() {
    int r,c;
    scanf("%d%d",&r,&c);
    int a[r][c];
    for (int i=0;i<r;i++) {
        for (int j=0;j<c;j++) {
            scanf("%d",&a[i][j]);
        }
    }
    int maxpro;
    int maxrow=0;
    for (int j=0;j<c;j++) {
        maxpro=maxpro*a[0][j];
    }
    for (int i=0;i<r;i++) {
        int pro=1;
        for (int j=0;j<c;j++) {
            pro=pro*a[i][j];
        }
        if (pro>maxpro) {
            maxpro=pro;
            maxrow=i;
        }
    }
    printf("%d",maxrow);
    return 0;
}
