#include <stdio.h>
int main() {
    int r, c;
    scanf("%d %d", &r, &c);
    int a[r][c];
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    int max= 0;
    int col = 0;
    for (int j = 0; j < c; j++) {
        int sum = 0;
        for (int i = 0; i < r; i++) {
            sum = sum + a[i][j];
        }
        if (j == 0) {
            max = sum;
            col = j;
        }
        if (sum > max) {
            max= sum;
            col= j;
        }
    }
    printf("%d", col);
    return 0;
}
