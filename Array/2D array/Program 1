#include<stdio.h>
int main() {
    int r,c;
    scanf("%d %d",&r,&c);
    int arr[r][c];
    for (int i=0;i<r;i++) {
        for (int j=0;j<c;j++) {
            scanf("%d",&arr[i][j]);
        }
    }
    int lar=0;
    int sec=0;
    for (int i=0;i<r;i++) {
        for (int j=0;j<c;j++) {
            if (arr[i][j]>lar) {
                sec=lar;
                lar=arr[i][j];
            }
            else if (arr[i][j]>sec&&arr[i][j]<lar) {
                sec=arr[i][j];
            }
        }
    }
    printf("%d\n",sec);
    return 0;
}
