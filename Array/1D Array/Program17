#include<stdio.h>
int main () {
    int n;
    scanf("%d", &n);
    int arr[n];
    for (int i=0; i<n; i++) {
        scanf("%d", &arr[i]);
    }
    int a=1;
    for (int i=0; i<n; i++) {
        if (arr[i]!=arr[n-1-i]) {
            a=0;
            break;
        }
    }
    if (a) {
        printf("Yes");
    }
    else {
        printf("No");
    }
    return 0;
}
