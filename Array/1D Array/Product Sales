#include <stdio.h>
int main() {
    int n;
    int sum=0;
    int count=0;
    scanf("%d",&n);
    int arr[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    for (int i=0;i<n;i++) {
        sum=sum+arr[i];
    }
    int avg=sum/n;
    for (int i=0;i<n;i++) {
        if (arr[i]>avg) {
            count++;
        }
    }
    printf("%d",count);
    //printf("%d",avg);
    return 0;
}
