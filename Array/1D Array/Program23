#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int a[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    int c = a[0];
    for (int i = 1; i < n; i++) {
        int x = a[i];
        int y = c;
        if (x < 0) {
            x = -x;
        }
        if (y < 0) {
            y = -y;
        }
        if (x < y) {
            c = a[i];
        }
        else {
            if (x == y) {
                if (a[i] > c) {
                    c = a[i];
                }
            }
        }
    }
    printf("%d", c);
    return 0;
}
