#include <stdio.h>
int main() {
    int size;
    scanf("%d", &size);
    int arr[size];
    for (int i = 0; i < size; i++)
        scanf("%d", &arr[i]);
    int largest = 0, second = 0;
        for (int i= 0; i < size; i++)
            {
                if (arr[i] > largest)
                {
                    second = largest;
                    largest = arr[i];
                }
        else if (arr[i] > second && arr[i] < largest)
            {
            second = arr[i];
            }
    }
    printf("%d\n", second);
    return 0;
}
