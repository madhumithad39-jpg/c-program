#include<stdio.h>
int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    int min=arr[0];
    int max=arr[1]-arr[0];
    for (int i=0;i<n;i++) {
        int pro=arr[i]-min;
        if (pro>max) {
            max=pro;
        }
        if (arr[i]<min) {
            min=arr[i];
        }
    }
    printf("%d",max);
    return 0;
}
