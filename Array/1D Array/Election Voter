#include<stdio.h>
int main() {
    int n;
    int total=0;
    scanf("%d",&n);
    int a[n];
    for (int i=0;i<n;i++) {
        scanf("%d",&a[i]);
    }
    for (int i=0;i<n;i++) {
        total=total+a[i];
    }
    int m=a[0];
    for (int i=1;i<n;i++) {
        if (a[i]>m) {
            m=a[i];
        }
    }
    printf("%d\n",total);
    printf("%d",m);
    return 0;
}
