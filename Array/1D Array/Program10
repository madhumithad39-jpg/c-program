#include<stdio.h>
int main() {
    int n;
    int a;
    int m=-1;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    for (int i=n-1;i>=0;i--) {
        a=arr[i];
        arr[i]=m;
        if (a>m) {
            m=a;
        }
    }
    for (int i=0;i<n;i++) {
        printf("%d",arr[i]);
    }
    return 0;
}
