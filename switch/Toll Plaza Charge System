#include <stdio.h>

int main() {
    int vehicleType, trips;
    scanf("%d %d", &vehicleType, &trips);

    switch(vehicleType) {
        case 1: 
            if(trips == 1)
                printf("100");
            else if(trips <= 10)
                printf("800");
            else
                printf("%d", trips * 100);
            break;

        case 2: 
            printf("%d", trips * 240);
            break;

        default: 
            printf("Invalid Vehicle Type");
            break;
    }

    return 0;
}
